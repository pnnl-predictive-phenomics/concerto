          <speciesReference species="M_hydroxide_e" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="M_bicarbonate_e" stoichiometry="1" constant="true"/>
          <speciesReference species="M_water_e" stoichiometry="1" constant="true"/>
        </listOfProducts>
      </reaction>
      <reaction id="R_reaction_dissociation_HCO3" reversible="true" fast="false" fbc:lowerFluxBound="cobra_default_lb" fbc:upperFluxBound="cobra_default_ub">
        <listOfReactants>
          <speciesReference species="M_bicarbonate_e" stoichiometry="1" constant="true"/>
          <speciesReference species="M_hydroxide_e" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="M_carbonate_e" stoichiometry="1" constant="true"/>
          <speciesReference species="M_water_e" stoichiometry="1" constant="true"/>
        </listOfProducts>
      </reaction>
      <reaction id="R_reaction_CaCO3_precipitation" reversible="true" fast="false" fbc:lowerFluxBound="cobra_default_lb" fbc:upperFluxBound="cobra_default_ub">
        <listOfReactants>
          <speciesReference species="M_carbonate_e" stoichiometry="1" constant="true"/>
          <speciesReference species="M_calcium_ion_e" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="M_calcium_carbonate_e" stoichiometry="1" constant="true"/>
        </listOfProducts>
      </reaction>
      <reaction metaid="meta_R_EX_urea_e" sboTerm="SBO:0000627" id="R_EX_urea_e" name="urea exchange" reversible="true" fast="false" fbc:lowerFluxBound="R_EX_urea_e_lower_bound" fbc:upperFluxBound="cobra_default_ub">
        <listOfReactants>
          <speciesReference species="M_urea_e" stoichiometry="1" constant="true"/>
        </listOfReactants>
      </reaction>
      <reaction metaid="meta_R_EX_ammonium_e" sboTerm="SBO:0000627" id="R_EX_ammonium_e" name="ammonium exchange" reversible="true" fast="false" fbc:lowerFluxBound="cobra_default_lb" fbc:upperFluxBound="R_EX_ammonium_e_upper_bound">
        <listOfReactants>
          <speciesReference species="M_ammonium_e" stoichiometry="1" constant="true"/>
        </listOfReactants>
      </reaction>
      <reaction metaid="meta_R_EX_calcium_ion_e" sboTerm="SBO:0000627" id="R_EX_calcium_ion_e" name="calcium_ion exchange" reversible="true" fast="false" fbc:lowerFluxBound="R_EX_calcium_ion_e_lower_bound" fbc:upperFluxBound="cobra_default_ub">
        <listOfReactants>
