          <speciesReference species="M_water_e" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="M_carbonic_acid_e" stoichiometry="1" constant="true"/>
        </listOfProducts>
      </reaction>
      <reaction id="R_reaction_dissociation_H2CO3" reversible="true" fast="false" fbc:lowerFluxBound="cobra_default_lb" fbc:upperFluxBound="cobra_default_ub">
